{
	"manifest_version": 3,
	"name": "template",
	"version": "0.0.1",
	"description": "template",
	"permissions": [],
	"host_permissions": [],
	"content_scripts": [
		{
			"matches": ["*"],
			"js": ["src/template.js"]
		}
	],
	"action": {
		"default_popup": "src/popup.html",
		"default_title": "template"
	},
	"icons": {
		"16": "assets/icon.png",
		"32": "assets/icon.png",
		"128": "assets/icon.png"
	}
}
